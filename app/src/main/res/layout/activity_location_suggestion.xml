<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".LocationSuggestion">

    <TextView
            android:layout_width="0dp"
            android:layout_height="0dp" android:id="@+id/CommonInterestsTextView"
            android:textSize="24sp"
            app:layout_constraintTop_toBottomOf="@+id/imageView2"
            app:layout_constraintStart_toEndOf="@+id/UserInfoTextView" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginBottom="11dp" app:layout_constraintBottom_toTopOf="@+id/textView7"
            android:layout_marginEnd="5dp"/>
    <TextView
            android:text="PresentNearbyResults"
            android:layout_width="188dp"
            android:layout_height="0dp" android:id="@+id/resultTextView"
            app:layout_constraintTop_toBottomOf="@+id/find_nearby_places" android:layout_marginBottom="153dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toTopOf="@+id/open_google_maps"
            app:layout_constraintStart_toStartOf="parent"/>
    <Button
            android:text="Open google maps"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/open_google_maps"
            app:layout_constraintStart_toStartOf="@+id/textView8" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/resultTextView" android:layout_marginBottom="37dp"/>
    <Button
            android:text="Find Nearby places"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/find_nearby_places"
            app:layout_constraintStart_toStartOf="@+id/resultTextView"
            android:layout_marginStart="11dp" app:layout_constraintTop_toBottomOf="@+id/textView7"
            app:layout_constraintBottom_toTopOf="@+id/resultTextView"/>
    <TextView
            android:text="You both like: "
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/UserInfoTextView"
            android:textSize="24sp"
            app:layout_constraintTop_toBottomOf="@+id/youMatched"
            app:layout_constraintEnd_toStartOf="@+id/CommonInterestsTextView" android:layout_marginBottom="465dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="16dp" app:layout_constraintBottom_toBottomOf="parent"/>
    <ImageView
            android:layout_width="180dp"
            android:layout_height="0dp" app:srcCompat="@drawable/_2ae7a8c6_9328_4e1b_a103_a5c78411f551"
            android:id="@+id/imageView2"
            app:layout_constraintTop_toBottomOf="@+id/youMatched" android:layout_marginBottom="17dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toTopOf="@+id/CommonInterestsTextView"
            app:layout_constraintStart_toStartOf="parent"/>
    <TextView
            android:text="You matched!"
            android:layout_width="214dp"
            android:layout_height="0dp" android:id="@+id/youMatched"
            android:textSize="34sp"
            android:layout_marginTop="25dp"
            android:layout_marginBottom="165dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/UserInfoTextView" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"/>
    <TextView
            android:text="Use this button below to get some suggestions on where to meet based on your interests: "
            android:layout_width="193dp"
            android:layout_height="0dp" android:id="@+id/textView7"
            app:layout_constraintTop_toBottomOf="@+id/CommonInterestsTextView" android:layout_marginBottom="1dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toTopOf="@+id/find_nearby_places"
            app:layout_constraintStart_toStartOf="parent"/>
    <TextView
            android:text="Or press this button to see nearby places yourself:"
            android:layout_width="165dp"
            android:layout_height="84dp" android:id="@+id/textView8"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toBottomOf="@+id/open_google_maps"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginBottom="47dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>