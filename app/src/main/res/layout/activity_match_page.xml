<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MatchPage">

    <Button
            android:id="@+id/matchmatchPageBtnCalcDistance"
            android:layout_width="282dp"
            android:layout_height="33dp"
            android:text="Get distance between users"
            android:background="@drawable/mybutton"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toBottomOf="@+id/matchPageText2"
            android:layout_marginBottom="134dp" app:layout_constraintBottom_toTopOf="@+id/matchPageText"/>
    <TextView
            android:text="TextView"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/matchPageText2"
            android:textAllCaps="true" android:textSize="24sp"
            android:layout_marginTop="86dp"
            android:layout_marginBottom="26dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/matchmatchPageBtnCalcDistance"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="19dp" android:layout_marginEnd="19dp"
    />
    <TextView
            android:text="TextView"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/matchPageText"
            android:textAllCaps="false" android:textSize="24sp"
            app:layout_constraintTop_toBottomOf="@+id/matchmatchPageBtnCalcDistance" android:layout_marginBottom="68dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginStart="26dp" android:layout_marginEnd="26dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>