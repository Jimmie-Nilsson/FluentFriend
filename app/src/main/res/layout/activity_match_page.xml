<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MatchPage">

    <Button
            android:id="@+id/matchmatchPageBtnCalcDistance"
            android:layout_width="288dp"
            android:layout_height="32dp"
            android:text="Get distance between users"
            android:background="@drawable/mybutton"
            android:textSize="16sp"
            app:layout_constraintTop_toBottomOf="@+id/matchPageText2" android:layout_marginBottom="167dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toTopOf="@+id/matchPageText"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="57dp" android:layout_marginEnd="57dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/matchPageText2"
            android:textAllCaps="true" android:textSize="20sp"
            android:layout_marginTop="86dp"
            android:layout_marginBottom="46dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/matchmatchPageBtnCalcDistance"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="46dp" android:layout_marginEnd="46dp"
    />
    <TextView
            android:text="TextView"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/matchPageText"
            android:textAllCaps="true" android:textSize="20sp"
            app:layout_constraintTop_toBottomOf="@+id/matchmatchPageBtnCalcDistance" android:layout_marginBottom="137dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginStart="51dp" android:layout_marginEnd="51dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>