<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".UserProfilePage" android:background="#BDE3FF" android:clickable="false"
>


    <ScrollView
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/scrollView2"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="1.0" app:layout_constraintVertical_bias="1.0">
        <androidx.constraintlayout.widget.ConstraintLayout android:layout_width="match_parent"
                                                           android:layout_height="wrap_content"
                                                           android:orientation="vertical"
                                                           android:id="@+id/linearLayout">
            <TextView
                    android:text="Gender:"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" android:id="@+id/genderTextView"
                    android:textSize="24sp"
                    app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_chainStyle="packed"
                    app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="321dp"
                    app:layout_constraintEnd_toStartOf="@+id/maleCheckBox"/>
            <CheckBox
                    android:text="Other/Private"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" android:id="@+id/otherCheckBox"
                    android:minHeight="48dp"
                    app:layout_constraintBaseline_toBaselineOf="@+id/femaleCheckBox"
                    app:layout_constraintStart_toEndOf="@+id/femaleCheckBox" android:layout_marginEnd="8dp"
                    app:layout_constraintEnd_toEndOf="parent"/>
            <CheckBox
                    android:text="Female"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" android:id="@+id/femaleCheckBox"
                    android:minHeight="48dp"
                    android:layout_marginTop="15dp"
                    app:layout_constraintTop_toBottomOf="@+id/displayUserNameTextView"
                    app:layout_constraintStart_toEndOf="@+id/maleCheckBox"
                    app:layout_constraintEnd_toStartOf="@+id/otherCheckBox"/>
            <CheckBox
                    android:text="Male"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" android:id="@+id/maleCheckBox"
                    android:minHeight="48dp"
                    app:layout_constraintBaseline_toBaselineOf="@+id/femaleCheckBox"
                    app:layout_constraintStart_toEndOf="@+id/genderTextView"
                    app:layout_constraintEnd_toStartOf="@+id/femaleCheckBox"/>
            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" android:id="@+id/displayUserNameTextView"
                    android:textSize="24sp"
                    android:text="displayUserName"
                    app:layout_constraintBaseline_toBaselineOf="@+id/userNameTextView" android:layout_marginEnd="19dp"
                    app:layout_constraintEnd_toEndOf="@+id/otherCheckBox"/>
            <AutoCompleteTextView
                    android:text="I want to learn..."
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" android:id="@+id/languesWantToLearnTextView"
                    android:minHeight="48dp"
                    app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginTop="24dp" android:layout_marginStart="34dp"
                    app:layout_constraintTop_toBottomOf="@+id/languesSpokenTextView"/>
            <TextView
                    android:text="Profile Settings"
                    android:textSize="20dp"
                    android:shadowColor="@color/text_shadow"
                    android:shadowDx="1"
                    android:shadowDy="1"
                    android:shadowRadius="2"
                    android:textColor="@color/black"
                    android:textAlignment="center"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" android:id="@+id/textView2"
                    app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="30dp"
                    android:layout_marginEnd="20dp"
                    app:layout_constraintEnd_toEndOf="@+id/imageView"/>
            <CheckBox
                    android:text="MUSEUM"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" android:id="@+id/museumCheckBox"
                    android:minHeight="48dp"
                    app:layout_constraintStart_toStartOf="@+id/fikaCheckBox"
                    android:layout_marginBottom="11dp" app:layout_constraintBottom_toTopOf="@+id/fikaCheckBox"/>
            <CheckBox
                    android:text="CITY WALK"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" android:id="@+id/cityWalksCheckBox"
                    android:minHeight="48dp"
                    android:layout_marginTop="38dp"
                    app:layout_constraintTop_toBottomOf="@+id/textView4"
                    app:layout_constraintEnd_toStartOf="@+id/museumCheckBox"
                    app:layout_constraintStart_toStartOf="@+id/guideline3"
                    android:layout_marginStart="63dp" android:layout_marginEnd="64dp"
            />
            <CheckBox
                    android:text="BAR"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" android:id="@+id/barCheckBox"
                    android:minHeight="48dp"
                    app:layout_constraintStart_toStartOf="@+id/cityWalksCheckBox"
                    android:layout_marginTop="11dp" app:layout_constraintTop_toBottomOf="@+id/cityWalksCheckBox"/>
            <CheckBox
                    android:text="FIKA"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" android:id="@+id/fikaCheckBox"
                    android:minHeight="48dp"
                    android:layout_marginEnd="139dp"
                    android:layout_marginBottom="10dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintBottom_toTopOf="@+id/saveSettingsButton"/>
            <TextView
                    android:text="My interests"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" android:id="@+id/textView4"
                    android:textSize="24sp"
                    app:layout_constraintStart_toStartOf="@+id/bioEditText" android:layout_marginBottom="38dp"
                    app:layout_constraintBottom_toTopOf="@+id/museumCheckBox"/>
            <TextView
                    android:text="About me"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" android:id="@+id/textView5"
                    android:textSize="24sp"
                    app:layout_constraintStart_toStartOf="@+id/bioEditText" android:layout_marginBottom="8dp"
                    app:layout_constraintBottom_toTopOf="@+id/bioEditText"/>
            <TextView
                    android:text="Namn:"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" android:id="@+id/userNameTextView"
                    android:textSize="24sp"
                    app:layout_constraintEnd_toStartOf="@+id/displayUserNameTextView" android:layout_marginEnd="17dp"
                    android:layout_marginBottom="22dp"
                    app:layout_constraintBottom_toTopOf="@+id/genderTextView"/>
            <AutoCompleteTextView
                    android:text="I speak..."
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" android:id="@+id/languesSpokenTextView"
                    android:minHeight="48dp"
                    app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginTop="34dp" android:layout_marginStart="32dp"
                    app:layout_constraintTop_toBottomOf="@+id/maleCheckBox"/>
            <Button
                    android:id="@+id/saveSettingsButton"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="SAVE SETTINGS"
                    android:background="@drawable/mybutton"
                    android:textSize="16sp"
                    app:layout_constraintBottom_toBottomOf="parent" android:layout_marginEnd="27dp"
                    app:layout_constraintEnd_toEndOf="@+id/fikaCheckBox"/>
            <androidx.constraintlayout.widget.Guideline android:layout_width="wrap_content"
                                                        android:layout_height="wrap_content"
                                                        android:id="@+id/guideline3"
                                                        android:orientation="vertical"
                                                        app:layout_constraintGuide_end="431dp"/>
            <EditText
                    android:layout_width="0dp"
                    android:layout_height="197dp"
                    android:inputType="text"
                    android:text="Biography..."
                    android:ems="10"
                    android:id="@+id/bioEditText"
                    android:gravity="top|start"
                    android:layout_marginTop="114dp" android:layout_marginBottom="116dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/languesWantToLearnTextView"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintBottom_toBottomOf="@+id/cityWalksCheckBox" android:layout_marginStart="32dp"
                    android:layout_marginEnd="32dp"/>
            <ImageView
                    android:layout_width="178dp"
                    android:layout_height="0dp" app:srcCompat="@drawable/_60152224_acb6_4f98_a275_c9d7e1cae641"
                    android:id="@+id/imageView"
                    android:layout_marginTop="45dp" android:layout_marginBottom="45dp"
                    app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="@+id/textView2"
                    app:layout_constraintStart_toStartOf="@+id/guideline3"
                    app:layout_constraintBottom_toBottomOf="@+id/displayUserNameTextView"
                    android:layout_marginStart="126dp" android:layout_marginEnd="126dp"
            />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>